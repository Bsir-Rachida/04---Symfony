<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<link rel="icon" href="/build/images/cinema.png">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`
				           and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<header>
		{% block navbar %}
			{{ include ('program/include/_navbar.html.twig')}}
		{% endblock %}
		{% block flash %}
			{% for category, messages in app.flashes %}

				{% for message in messages %}

					<div class="alert alert-{{ category }}">

						{{ message }}

					</div>

				{% endfor %}

			{% endfor %}

		{% endblock %}

	</header<body>
	{% block body %}{% endblock %}
</html></body></html>
